Algoritmo sin_titulo
	//Crear dos vectores uno que almacene 2 nombres y otro que almacene 3 números
	
	Dimension nombres[100]
	Dimension numer[100]
	
	mostrar "ingrese cantidad de nombres "
	leer n
	mostrar "ingrese cantidad de numeros "
	leer a
	
	para i = 1 hasta n
		
		mostrar " ingrese nombre: "
		leer nombres[i]
		
	FinPara
	
	para j = 1 hasta a
		Mostrar " ingrese numero: "
		leer numer[j]
	FinPara
	
	
	mostrar numer[1]
	mostrar numer[2]
	
	
	
FinAlgoritmo
